CREATE MULTISET TABLE {{env}}_stg_t.V3D_ACCOUNTS_LOG ,FALLBACK ,
     NO BEFORE JOURNAL,
     NO AFTER JOURNAL,
     CHECKSUM = DEFAULT,
     DEFAULT MERGEBLOCKRATIO,
     MAP = TD_MAP1
     (
      LOG_ID INTEGER NOT NULL GENERATED ALWAYS AS IDENTITY
           (START WITH 1 
            INCREMENT BY 1 
            MINVALUE -2147483647 
            MAXVALUE 2147483647 
            NO CYCLE),
      ACCOUNT_ID INTEGER,
      OPERATION VARCHAR(10) CHARACTER SET LATIN CASESPECIFIC,
      OPERATION_DATE TIMESTAMP(6),
      OLD_ACCOUNT_TYPE VARCHAR(50) CHARACTER SET LATIN CASESPECIFIC,
      NEW_ACCOUNT_TYPE VARCHAR(50) CHARACTER SET LATIN CASESPECIFIC,
      OLD_BALANCE DECIMAL(18,2),
      NEW_BALANCE DECIMAL(18,2),
      OLD_STATUS VARCHAR(20) CHARACTER SET LATIN CASESPECIFIC,
      NEW_STATUS VARCHAR(20) CHARACTER SET LATIN CASESPECIFIC, 
PRIMARY KEY ( LOG_ID ))

;
