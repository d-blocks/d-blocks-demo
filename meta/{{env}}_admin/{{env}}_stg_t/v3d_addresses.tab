CREATE MULTISET TABLE {{env}}_stg_t.V3D_ADDRESSES ,FALLBACK ,
     NO BEFORE JOURNAL,
     NO AFTER JOURNAL,
     CHECKSUM = DEFAULT,
     DEFAULT MERGEBLOCKRATIO,
     MAP = TD_MAP1
     (
      ADDRESS_ID INTEGER NOT NULL,
      CUSTOMER_ID INTEGER,
      STREET_ADDRESS VARCHAR(255) CHARACTER SET LATIN CASESPECIFIC,
